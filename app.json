{
  "expo": {
    "name": "sportZona7",
    "slug": "sportZona7",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Welcome/images.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bastsrojasexpo.appstore"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Unbounded.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "04a7fe74-cb20-48d2-82c6-5f842fc45ace"
      }
    }
  }
}
